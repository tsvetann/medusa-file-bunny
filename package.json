{
  "name": "medusa-file-bunny",
  "version": "0.0.4",
  "description": "bunny.net file connector for Medusa",
  "author": "Tsvetan Nikolov <live.webscore@gmail.com>",
  "license": "MIT",
  "keywords": [
    "ecommerce",
    "medusa",
    "medusa-plugin",
    "bunny.net",
    "medusa-plugin-storage"
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "babel src --out-dir . --ignore **/__tests__ --extensions \".ts,.js\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir ./dist --ignore '**/__tests__','**/__mocks__'",
    "test": "cross-env jest",
    "start:custom": "cross-env npm run build && node --preserve-symlinks index.js",
    "dev": "cross-env npm run build && medusa develop",
    "build:admin": "cross-env medusa-admin build"
  },
  "main": "services/bunny-file.js",
  "files": [
    "services"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tsvetann/medusa-file-bunny.git"
  },
  "dependencies": {
    "node-fetch": "^2.6.11",
    "stream": "^0.0.2"
  },
  "peerDependencies": {
    "@medusajs/medusa": ">= 1.16.1 < 2",
    "medusa-interfaces": "^1.3.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@medusajs/cache-redis": "^1.8.7",
    "@medusajs/event-bus-local": "^1.9.4",
    "@medusajs/event-bus-redis": "^1.8.7",
    "@medusajs/medusa": "1.17.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "babel-preset-medusa-package": "^1.1.13",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "express": "^4.17.2",
    "jest": "^29.7.0",
    "medusa-interfaces": "^1.3.7",
    "mongoose": "^5.13.14",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.4.3",
    "typeorm": "npm:@medusajs/typeorm@next",
    "typescript": "^4.5.2"
  }
}
